{
  "description": "股票标签黑名单正则表达式列表",
  "patterns": [
    {
      "category": "减持相关",
      "patterns": [
        "^.*减持.*$",
        "^股东减持$",
        "^减持公告$",
        "^股东减持公告$"
      ]
    },
    {
      "category": "报告披露",
      "patterns": [
        "^\\d{4}年.*报告.*$",
        "^.*半年.*报告.*$",
        "^.*年度报告.*$",
        "^.*季度报告.*$",
        "^半年报.*$",
        "^年报.*$",
        "^中报.*$",
        "^.*报告披露$",
        "^.*报告发布$",
        "^中报发布$"
      ]
    },
    {
      "category": "股东大会",
      "patterns": [
        "^.*股东大?会.*$",
        "^临时股东大会$",
        "^临时股东会$"
      ]
    },
    {
      "category": "股份回购",
      "patterns": [
        "^股份回购.*$",
        "^.*回购公告$"
      ]
    },
    {
      "category": "业绩说明会",
      "patterns": [
        "^.*业绩说明会$",
        "^\\d{4}年.*业绩说明会$"
      ]
    },
    {
      "category": "投资者互动",
      "patterns": [
        "^投资者互动平台.*$",
        "^互动平台.*$",
        "^投资者互动问答$",
        "^.*互动.*问答.*$"
      ]
    },
    {
      "category": "龙虎榜",
      "patterns": [
        "^龙虎榜.*$"
      ]
    },
    {
      "category": "公告类",
      "patterns": [
        "^.*权益分派公告$",
        "^股票交易异常波动公告$",
        "^.*公告$"
      ]
    },
    {
      "category": "会议活动",
      "patterns": [
        "^董事会会议$",
        "^监事会会议$",
        "^监事会决议$",
        "^职工代表大会$",
        "^职工代表董事选举$",
        "^选举职工代表董事$",
        "^职工代表大会选举$",
        "^职工董事选举$",
        "^聘任证券事务代表$",
        "^.*选举.*$",
        "^.*监事会.*$"
      ]
    },
    {
      "category": "资本运作",
      "patterns": [
        "^资本运作$",
        "^并购$",
        "^收购$",
        "^股权转让$",
        "^股权激励$",
        "^限制性股票激励计划$",
        "^员工持股计划$",
        "^.*股票激励.*$",
        "^.*持股计划.*$",
        "^大宗交易$",
        "^限售股解禁$",
        "^资产重组$",
        "^利润分配$",
        "^分红$",
        "^现金分红$",
        "^.*分红.*$",
        "^.*解禁.*$",
        "^担保$",
        "^无逾期担保$",
        "^.*担保.*$"
      ]
    },
    {
      "category": "公司治理",
      "patterns": [
        "^公司治理$",
        "^工商变更登记$"
      ]
    },
    {
      "category": "交易所及资金流向",
      "patterns": [
        "^.*证券交易所$",
        "^主力资金.*$",
        "^资金流出$",
        "^.*资金流.*$"
      ]
    },
    {
      "category": "业绩相关",
      "patterns": [
        "^业绩亏损$",
        "^业绩下滑$",
        "^业绩报告$",
        "^业绩增长$",
        "^净利润增长$",
        "^亏损$",
        "^.*业绩.*$",
        "^.*亏损.*$",
        "^.*利润增长.*$"
      ]
    },
    {
      "category": "交易异常",
      "patterns": [
        "^涨停$",
        "^跌停$",
        "^股票交易异常波动$",
        "^股票异常波动$",
        "^异常波动$",
        "^.*交易异常.*$",
        "^.*异常波动.*$",
        "^筹码集中$"
      ]
    },
    {
      "category": "机构活动",
      "patterns": [
        "^机构调研$",
        "^关联交易$"
      ]
    },
    {
      "category": "股东信息",
      "patterns": [
        "^股东户数披露$",
        "^股东户数下降$",
        "^股东人数披露$",
        "^.*股东户数.*$",
        "^.*股东人数.*$"
      ]
    },
    {
      "category": "概念性标签",
      "patterns": [
        "^高质量发展$",
        "^数字化转型$",
        "^产业升级$"
      ]
    },
    {
      "category": "高管变动",
      "patterns": [
        "^董事辞职$",
        "^非独立董事辞职$",
        "^高管变动$",
        "^.*董事辞职$",
        "^.*高管变动.*$"
      ]
    }
  ],
  "combined_patterns": [
    "^.*减持.*$",
    "^\\d{4}年.*(报告|业绩说明会|半年报).*$",
    "^.*(半年|年度|季度|中).*(报告|业绩).*$",
    "^(中报发布|中报|半年报披露)$",
    "^.*股东(大会|会)$",
    "^股份回购.*$",
    "^投资者互动平台.*$",
    "^互动平台.*$",
    "^.*互动.*问答.*$",
    "^龙虎榜.*$",
    "^.*公告$",
    "^(董事会会议|监事会会议|监事会决议)$",
    "^.*监事会.*$",
    "^.*选举.*$",
    "^职工代表(大会|董事选举)$",
    "^(选举职工代表董事|职工代表大会选举|职工董事选举|聘任证券事务代表)$",
    "^(资本运作|并购|收购|股权转让|股权激励|大宗交易|公司治理|工商变更登记)$",
    "^(限售股解禁|资产重组|利润分配|分红|现金分红|限制性股票激励计划|员工持股计划)$",
    "^.*解禁.*$",
    "^.*分红.*$",
    "^.*股票激励.*$",
    "^.*持股计划.*$",
    "^.*担保.*$",
    "^.*证券交易所$",
    "^主力资金.*$",
    "^.*资金流.*$",
    "^(业绩亏损|业绩下滑|业绩报告|业绩增长|净利润增长|亏损)$",
    "^.*业绩.*$",
    "^.*亏损.*$",
    "^.*利润增长.*$",
    "^(涨停|跌停)$",
    "^(股票交易异常波动|股票异常波动|异常波动)$",
    "^.*交易异常.*$",
    "^.*异常波动.*$",
    "^筹码集中$",
    "^(机构调研|关联交易)$",
    "^(股东户数披露|股东户数下降|股东人数披露)$",
    "^.*股东户数.*$",
    "^.*股东人数.*$",
    "^(高质量发展|数字化转型|产业升级)$",
    "^(董事辞职|非独立董事辞职|高管变动)$",
    "^.*董事辞职$",
    "^.*高管变动.*$"
  ]
}

